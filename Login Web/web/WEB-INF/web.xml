<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <context-param>
        <param-name>encoding</param-name>
        <param-value>utf-8</param-value>
    </context-param>

    <servlet>
        <servlet-name>signUpServe</servlet-name>
        <servlet-class>webservlet.signUpServe</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signUpServe</servlet-name>
        <url-pattern>/signUpServe</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>loginServe</servlet-name>
        <servlet-class>webservlet.loginServe</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServe</servlet-name>
        <url-pattern>/loginServe</url-pattern>
    </servlet-mapping>
</web-app>